{
  "name": "adnbn",
  "version": "0.0.32",
  "description": "Addon ðŸ’€ Bone - Cross browser web extension framework with shared code base",
  "homepage": "https://github.com/addonbone/addonbone",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/addonbone/addonbone.git"
  },
  "keywords": [
    "addon",
    "extension",
    "chrome",
    "firefox",
    "edge",
    "opera",
    "safari"
  ],
  "author": "AddonBone",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/addonbone/addonbone/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./browser": {
      "types": "./dist/browser/index.d.ts",
      "import": "./dist/browser/index.js",
      "require": "./dist/browser/index.cjs"
    },
    "./entry/*": {
      "types": "./dist/entry/*/index.d.ts",
      "import": "./dist/entry/*/index.js",
      "require": "./dist/entry/*/index.cjs"
    },
    "./entry/content/*": {
      "types": "./dist/entry/content/*.d.ts",
      "import": "./dist/entry/content/*.js",
      "require": "./dist/entry/content/*.cjs"
    },
    "./client-types": {
      "types": "./types/client.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "bin",
    "dist",
    "types"
  ],
  "bin": {
    "adnbn": "bin/adnbn.js"
  },
  "scripts": {
    "adnbn": "tsx ./src/cli/index.ts",
    "build": "tsup"
  },
  "devDependencies": {
    "@swc/core": "^1.11.7",
    "@types/lodash": "^4.17.14",
    "@types/node": "^22.13.1",
    "@types/pluralize": "^0.0.33",
    "dotenv": "^16.4.7",
    "esbuild-plugin-raw": "^0.1.8",
    "fs-extra": "^11.3.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "utility-types": "^3.11.0",
    "uglify-js": "^3.19.3"
  },
  "dependencies": {
    "@rsdoctor/rspack-plugin": "^0.4.13",
    "@rspack/cli": "^1.2.7",
    "@rspack/core": "^1.2.7",
    "@types/chrome": "^0.0.307",
    "c12": "^2.0.1",
    "cac": "^6.7.14",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "json-stringify-deterministic": "^1.0.12",
    "just-kebab-case": "^4.2.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.9.2",
    "nanoid": "^5.1.4",
    "pluralize": "^8.0.0",
    "rspack-plugin-virtual-module": "^0.1.13",
    "sass": "^1.83.4",
    "sass-loader": "^16.0.4",
    "source-modifier-loader": "^1.0.2",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^4.37.0",
    "typescript": "^5.7.3",
    "webpack-merge": "^6.0.1",
    "zod": "^3.24.2"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": "^16.8 || ^17.0 || ^18.0 || ^19.0 || ^19.0.0-rc",
    "react-dom": "^16.8 || ^17.0 || ^18.0 || ^19.0 || ^19.0.0-rc"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  }
}
