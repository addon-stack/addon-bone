{
  "name": "adnbn",
  "version": "0.0.15",
  "description": "Cross browser web extension framework with shared code base",
  "homepage": "https://github.com/addonbone/addonbone",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/addonbone/addonbone.git"
  },
  "keywords": [
    "addon",
    "extension",
    "chrome",
    "firefox",
    "edge",
    "opera",
    "safari"
  ],
  "author": "AddonBone",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/addonbone/addonbone/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "bin",
    "dist"
  ],
  "bin": {
    "adnbn": "bin/adnbn.js"
  },
  "scripts": {
    "adnbn": "tsx ./src/cli/index.ts",
    "build": "tsup"
  },
  "devDependencies": {
    "@types/generate-json-webpack-plugin": "^0.3.7",
    "@types/lodash": "^4.17.14",
    "@types/node": "^22.13.1",
    "dotenv": "^16.4.7",
    "fs-extra": "^11.3.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2"
  },
  "dependencies": {
    "@types/chrome": "^0.0.299",
    "c12": "^2.0.1",
    "cac": "^6.7.14",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "generate-json-webpack-plugin": "^2.0.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.9.2",
    "sass": "^1.83.4",
    "sass-loader": "^16.0.4",
    "ts-loader": "^9.5.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.7.3",
    "utility-types": "^3.11.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-virtual-modules": "^0.6.2"
  }
}
