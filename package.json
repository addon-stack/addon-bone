{
  "name": "adnbn",
  "version": "0.0.31",
  "description": "Cross browser web extension framework with shared code base",
  "homepage": "https://github.com/addonbone/addonbone",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/addonbone/addonbone.git"
  },
  "keywords": [
    "addon",
    "extension",
    "chrome",
    "firefox",
    "edge",
    "opera",
    "safari"
  ],
  "author": "AddonBone",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/addonbone/addonbone/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./browser": {
      "types": "./dist/browser/index.d.ts",
      "import": "./dist/browser/index.js",
      "require": "./dist/browser/index.cjs"
    },
    "./client/background": {
      "types": "./dist/client/background.d.ts",
      "import": "./dist/client/background.js",
      "require": "./dist/client/background.cjs"
    },
    "./client/command": {
      "types": "./dist/client/command.d.ts",
      "import": "./dist/client/command.js",
      "require": "./dist/client/command.cjs"
    },
    "./client/content": {
      "types": "./dist/client/content.d.ts",
      "import": "./dist/client/content.js",
      "require": "./dist/client/content.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "bin",
    "dist"
  ],
  "bin": {
    "adnbn": "bin/adnbn.js"
  },
  "scripts": {
    "adnbn": "tsx ./src/cli/index.ts",
    "build": "tsup"
  },
  "devDependencies": {
    "@swc/core": "^1.11.7",
    "@types/lodash": "^4.17.14",
    "@types/node": "^22.13.1",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "dotenv": "^16.4.7",
    "esbuild-plugin-raw": "^0.1.8",
    "fs-extra": "^11.3.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "uglify-js": "^3.19.3"
  },
  "dependencies": {
    "@rsdoctor/rspack-plugin": "^0.4.13",
    "@rspack/cli": "^1.2.7",
    "@rspack/core": "^1.2.7",
    "@types/chrome": "^0.0.307",
    "c12": "^2.0.1",
    "cac": "^6.7.14",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "json-stringify-deterministic": "^1.0.12",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.9.2",
    "pluralize": "^8.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rspack-plugin-virtual-module": "^0.1.13",
    "sass": "^1.83.4",
    "sass-loader": "^16.0.4",
    "source-modifier-loader": "^1.0.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "utility-types": "^3.11.0",
    "webpack-merge": "^6.0.1",
    "zod": "^3.24.2"
  }
}
